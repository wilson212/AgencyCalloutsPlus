    <Name>Los Santos Police Department</Name>
    <ScriptName>lspd</ScriptName>
    <AgencyType>CityPolice</AgencyType>
    <Vehicles>
      <LocalPatrol>
        <Vehicle probability="5">POLICE</Vehicle>
        <Vehicle probability="5">POLICE2</Vehicle>
        <Vehicle probability="5">POLICE3</Vehicle>
      </LocalPatrol>
      <Detectives>
